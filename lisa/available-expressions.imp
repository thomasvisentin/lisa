class availableExpressions {

	example() {
	    def a = 2;
	    def b = 2;
		def x = a+b;
		def y = a*b;
		while(y > a+b){
		    a = a+1;
		    x = a+b;
		}
	}

	test2() {
		def x = 0;
		def i = 0;
